{
  "name": "@mymcp/cli",
  "version": "0.1.0",
  "type": "commonjs",
  "description": "myMCP Command Line Interface - Fantasy-themed chatbot CLI",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "mycli": "dist/mycli.js"
  },
  "files": [
    "dist/**/*",
    "src/**/*",
    "bin/**/*"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "nodemon --exec \"npm run build && node dist/index.js\"",
    "start": "node dist/index.js",
    "test": "jest",
    "clean": "node -e \"const fs=require('fs'); if(fs.existsSync('./dist')) fs.rmSync('./dist',{recursive:true,force:true})\"",
    "prebuild": "npm run clean",
    "build:full": "node build.js build",
    "install-global": "npm link",
    "shell": "node interactive-shell.js"
  },
  "dependencies": {
    "@mymcp/types": "file:../../shared/types",
    "commander": "^11.0.0",
    "axios": "^1.6.0",
    "chalk": "^4.1.2",
    "figlet": "^1.7.0",
    "inquirer": "^8.2.6"
  },
  "devDependencies": {
    "@types/figlet": "^1.5.0",
    "@types/inquirer": "^8.2.0",
    "jest": "^29.0.0",
    "nodemon": "^3.0.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "cli",
    "chatbot",
    "fantasy",
    "mcp",
    "typescript"
  ]
}