{
  "name": "@mymcp/admin",
  "version": "0.1.0",
  "description": "myMCP Admin Dashboard - Quest editor and system monitoring interface",
  "main": "dist/index.js",
  "scripts": {
    "build": "npm run build:server && npm run build:client",
    "build:server": "tsc",
    "build:client": "npm run build --prefix client",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "nodemon --exec \"npm run build:server && node dist/index.js\"",
    "dev:client": "npm start --prefix client",
    "start": "node dist/index.js",
    "test": "jest",
    "clean": "rm -rf dist && rm -rf client/build",
    "setup": "npm install && cd client && npm install"
  },
  "dependencies": {
    "@mymcp/types": "file:../../shared/types",
    "express": "^4.18.0",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "socket.io": "^4.7.0",
    "uuid": "^9.0.0",
    "node-fetch": "^3.3.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.0",
    "@types/cors": "^2.8.0",
    "@types/morgan": "^1.9.0",
    "@types/uuid": "^9.0.0",
    "@types/node": "^18.0.0",
    "jest": "^29.0.0",
    "nodemon": "^3.0.0",
    "typescript": "^5.0.0",
    "concurrently": "^8.0.0"
  }
}