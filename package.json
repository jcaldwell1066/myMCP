{
  "name": "mymcp",
  "version": "0.1.0",
  "description": "myMCP Lunch and Learn Project - Multi-interface chatbot system for lunch and learn demo",
  "private": true,
  "workspaces": [
    "packages/*",
    "shared/*"
  ],
  "scripts": {
    "build": "npm run build --workspaces",
    "dev": "npm run dev --workspaces --parallel",
    "start": "npm run start --workspaces",
    "test": "npm run test --workspaces",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "clean": "rm -rf node_modules packages/*/node_modules packages/*/dist shared/*/node_modules shared/*/dist",
    "dev:engine": "npm run dev --workspace=packages/engine",
    "dev:cli": "npm run dev --workspace=packages/cli",
    "dev:webapp": "npm run dev --workspace=packages/webapp",
    "dev:admin": "npm run dev --workspace=packages/admin",
    "dev:mcpserver": "npm run dev --workspace=packages/mcpserver",
    "dev:slack": "npm run dev --workspace=packages/slack-integration",
    "cli:get-score": "npm run build --workspace=packages/cli && node packages/cli/dist/index.js get-score",
    "cli:set-score": "npm run build --workspace=packages/cli && node packages/cli/dist/index.js set-score",
    "dev:multiplayer": "docker-compose -f docker-compose.multiplayer.yml up",
    "dev:multiplayer:build": "docker-compose -f docker-compose.multiplayer.yml build",
    "dev:multiplayer:down": "docker-compose -f docker-compose.multiplayer.yml down",
    "dev:engine:primary": "ENGINE_ID=primary IS_PRIMARY=true PORT=3000 npm run dev:engine",
    "dev:engine:worker1": "ENGINE_ID=worker1 PORT=3001 npm run dev:engine",
    "dev:engine:worker2": "ENGINE_ID=worker2 PORT=3002 npm run dev:engine",
    "dev:engine:worker3": "ENGINE_ID=worker3 PORT=3003 npm run dev:engine",
    "redis:start": "docker run -d -p 6379:6379 --name mymcp-redis redis:7-alpine",
    "redis:stop": "docker stop mymcp-redis && docker rm mymcp-redis",
    "slack:setup": "cd packages/slack-integration && cp .env.example .env && echo 'Please edit packages/slack-integration/.env with your Slack credentials'",
    "start:admin": "node tools/startup/start-admin.js",
    "start:full": "node tools/startup/start-full-system.js",
    "build:all": "npm run build --workspaces",
    "dev:branch": "node tools/trunk-dev.js branch",
    "dev:sync": "node tools/trunk-dev.js sync",
    "dev:commit": "node tools/trunk-dev.js commit",
    "dev:feature": "node tools/trunk-dev.js feature",
    "dev:status": "node tools/trunk-dev.js status",
    "dev:rollback": "node tools/trunk-dev.js rollback",
    "dev:help": "node tools/trunk-dev.js help",
    "demo:setup": "node tools/demo-setup.js"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.0",
    "jest": "^29.0.0",
    "nodemon": "^3.0.0",
    "prettier": "^3.0.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/myMCP"
  },
  "keywords": [
    "chatbot",
    "mcp",
    "cli",
    "webapp",
    "typescript",
    "node",
    "lunch-learn",
    "demo",
    "slack",
    "integration"
  ],
  "author": "Accesso Payments Breakaway Group",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/server-memory": "^2025.4.25",
    "@modelcontextprotocol/server-sequential-thinking": "^0.6.2",
    "@socket.io/redis-adapter": "^8.3.0",
    "dotenv": "^16.5.0",
    "ioredis": "^5.6.1",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1"
  }
}
